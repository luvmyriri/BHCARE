{
    "css.validate": true,
    "css.lint.unknownAtRules": "ignore",
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
        "strings": true
    },
    "tailwindCSS.experimental.classRegex": [
        [
            "[a-zA-Z]*ClassName['\"`]\\s*:\\s*['\"`]([^'\"`]*)['\"`]",
            "([a-zA-Z0-9\\-:]+)"
        ]
    ]
}